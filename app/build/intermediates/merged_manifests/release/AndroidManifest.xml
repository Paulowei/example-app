<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.moderneinstein.android.examples"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="29"
        android:targetSdkVersion="31" />

    <application
        android:extractNativeLibs="false"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_title" >
        <activity
            android:name="com.moderneinstein.android.examples.CentralActivity"
            android:exported="true"
            android:label="@string/app_title" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.moderneinstein.android.examples.DisplayActivity"
            android:exported="true"
            android:parentActivityName="com.moderneinstein.android.examples.CentralActivity" >
        </activity>
    </application>

</manifest>